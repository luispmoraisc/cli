{
  "name": "vjscli",
  "version": "0.0.1",
  "description": "Vanilla JavaScript CLI",
  "keywords": [
    "cli",
    "es6",
    "ecmascript",
    "vanilla",
    "webpack",
    "front end"
  ],
  "main": "./src/index.js",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "screate": "npm run build && npm run stest",
    "build": "tsc -p .",
    "test": "npm i -g && vjs",
    "stest": "sudo npm i -g && vjs",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luispmoraisc/cli"
  },
  "author": "Luis Paulo Morais Cardoso",
  "contributors": [
    "Lennon Vinicius Alves Dias"
  ],
  "license": "MIT",
  "bin": {
    "vjs": "./lib/index.js"
  },
  "bugs": "https://github.com/luispmoraisc/cli/issues",
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "commander": "^3.0.2",
    "figlet": "^1.2.4",
    "fs": "0.0.1-security",
    "minimist": "^1.2.0",
    "ora": "^4.0.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/node": "^12.7.12",
    "nodemon": "^1.19.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  }
}
